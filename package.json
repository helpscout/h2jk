{
  "name": "h2jk",
  "version": "0.0.1",
  "description": "HubSpot to Jekyll",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:unit": "mocha test/setup.mocha.js --recursive lib/**/*.spec.js",
    "test:coverage": "nyc --reporter=html --reporter=text mocha test/setup.mocha.js --recursive lib/**/*.spec.js --reporter=dot",
    "pretty": "prettier '{lib,test}/**/*.js' --single-quote --trailing-comma es5 --write",
    "test": "npm run pretty && npm run test:unit",
    "test:watch": "npm run pretty && mocha test/setup.mocha.js --recursive lib/**/*.spec.js --watch --reporter dot"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "mocha": "3.4.2",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "10.3.2",
    "prettier": "1.5.2"
  },
  "author": "ItsJonQ <itsjonq@gmail.com> (http://jonquach.com)",
  "license": "MIT"
}
